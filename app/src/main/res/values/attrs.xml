<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- This is declaring the XML attributes that can be set within a theme. -->
    <!-- Check the "res/values/themes.xml" file for examples of these attributes being set.  -->
    <attr name="myColorPrimary" format="reference"/>
    <attr name="myColorPrimaryDark" format="reference"/>
    <attr name="myColorAccent" format="reference"/>

    <attr name="mainBackground" format="reference" />
    <attr name="headerBackground" format="reference" />
    <attr name="toolbarBackground" format="reference" />

    <attr name="textName" format="reference" />
    <attr name="textEmail" format="reference" />
    <attr name="textRegular" format="reference" />

    <attr name="tvToolbar" format="reference" />
    <attr name="tvSignin" format="reference" />
    <attr name="ibCreate" format="reference" />
    <attr name="ibBack" format="reference" />
    <attr name="tvContacts" format="reference" />
    <attr name="ibTrailer1" format="reference" />
    <attr name="ibTrailer2" format="reference" />
    <attr name="tvTrailer" format="reference" />
    <attr name="tvPoster" format="reference" />
    <attr name="gridViewStyling" format="reference"/>
    <attr name="ivOverlay" format="reference"/>
    <attr name="ibCancelTrailer" format="reference"/>

    <declare-styleable name="TicTacToeTile">
        <attr name="borderLeft" format="boolean" />
        <attr name="borderRight" format="boolean" />
        <attr name="borderTop" format="boolean" />
        <attr name="borderBottom" format="boolean" />
        <attr name="row" format="integer" />
        <attr name="col" format="integer" />
    </declare-styleable>

</resources>

